   "source": [
    "from qiskit.circuit import classical_function,  Int1\n",
    "\n",
    "@classical_function\n",
    "def grover_oracle(a: Int1, b: Int1, c: Int1, d: Int1) -> Int1:\n",
    "    return (not a and b and not c and d)\n",
    "\n",
    "quantum_circuit = grover_oracle.synth()\n",
    "quantum_circuit.draw()"
