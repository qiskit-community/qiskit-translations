msgstr "или младший кубит"

#: ../../tutorials/circuits/3_summary_of_quantum_operations.ipynb:2248
msgid "C_Y =\n"
"\\begin{pmatrix}\n"
"1 & 0 & 0 & 0\\\\\n"
"0 & 0 & 0 & -i\\\\\n"
