#: ../../tutorials/circuits/3_summary_of_quantum_operations.ipynb:918
msgid "Hadamard gate"
msgstr "ஹாட்மார்ட் கேட்"

#: ../../tutorials/circuits/3_summary_of_quantum_operations.ipynb:920
msgid "H =\n"
"\\frac{1}{\\sqrt{2}}\n"
"\\begin{pmatrix}\n"
"1 & 1\\\\\n"
"1 & -1\n"
"\\end{pmatrix}= u(\\pi/2,0,\\pi)"
msgstr "H =\n"
"\\frac{1}{\\sqrt{2}}\n"
"\\begin{pmatrix}\n"
"1 & 1\\\\\n"
"1 & -1\n"
"\\end{pmatrix}= u(\\pi/2,0,\\pi)"

