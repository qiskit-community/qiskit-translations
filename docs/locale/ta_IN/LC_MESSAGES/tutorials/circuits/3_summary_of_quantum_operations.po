msgstr ""

#: ../../tutorials/circuits/3_summary_of_quantum_operations.ipynb:2107
msgid "or when the LSB is the control"
msgstr "அல்லது LSB கட்டுப்பாடு பொழுது"

#: ../../tutorials/circuits/3_summary_of_quantum_operations.ipynb:2109
msgid "C_Y =\n"
"\\begin{pmatrix}\n"
"1 & 0 & 0 & 0\\\\\n"
"0 & 0 & 0 & -i\\\\\n"
"0 & 0 & 1 & 0\\\\\n"
"0 & i & 0 & 0\n"
"\\end{pmatrix}."
msgstr ""

