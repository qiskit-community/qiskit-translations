"  a_{k_l} | \\cdots, 0_1 \\cdots 1_{k_l} \\cdots 0_{N_l}, \\cdots\\rangle\n"
"   &= | \\cdots, 0_1 \\cdots 0_{k_l} \\cdots 0_{N_l}, \\cdots\\rangle \\\\\n"
"  a_{k_l} | \\cdots, 0_1 \\cdots 0_{k_l} \\cdots 0_{N_l}, \\cdots\\rangle &=  0 \\\\\n"
"\\end{aligned}"
